{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "ui": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "coverage": {
      "dependsOn": ["build", "test"],
      "outputs": []
    },
    "typedoc": {
      "dependsOn": ["build", "test"],
      "outputs": []
    }
  }
}
